{
  "name": "hapi-path-generator",
  "version": "0.0.18",
  "description": "A route generator for Hapi",
  "main": "dist/index.js",
  "repository": {
    "type": "private"
  },
  "scripts": {
    "testCI": "npm install && typings install && tsc --outDir dist && mocha ./test/ --recursive",
    "test": "npm install && nodemon -L -R min -e ts,js,json --ignore ./dist --exec 'rm -fr dist && tsc --outDir dist && mocha --debug -c -u bdd ./test/ --recursive'",
    "cov": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./test/ --recursive && cat ./coverage/lcov.info | ./node_modules/.bin/codacy-coverage"
  },
  "author": "Ruben Paz",
  "license": "ISC",
  "devDependencies": {
    "chai": "^3.5.0",
    "hapi": "^13.4.1",
    "hapi-sequelize": "^2.2.4",
    "mocha": "^2.5.3",
    "nodemon": "^1.9.2",
    "pg": "^5.0.0",
    "sequelize": "^3.23.3",
    "shot": "^3.1.0",
    "typescript": "^1.8.10",
    "typings": "^1.3.0"
  },
  "peerDependencies": {
    "sequelize": "3.x"
  },
  "dependencies": {
    "boom": "^3.2.2",
    "ramda": "^0.21.0",
    "validator": "^5.4.0"
  }
}
